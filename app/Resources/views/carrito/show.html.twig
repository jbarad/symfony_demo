						</div>
						<h3 class="step-1 heading">
							<span>1.</span> Por favor, indicanos los datos de los pasajeros
						</h3>
						
            {% for i in 1..pasajeros %}
            <fieldset class="adult passengers-info">
              <legend class="heading">Pasajero {{ i }} (Adulto)</legend>
              <ul>
                <li class="name">
                  <label>Nombre</label>
                  {{ form_errors(form["pax_" ~ i]["name"]) }}
                  {{ form_widget(form["pax_" ~ i]["name"]) }}
                  {% if ( errores_checkout['pax_' ~ i ~ '_name'] is defined ) %}
                    <div class="error ltr active">
                      <span class="left"></span><span class="right">{{errores_checkout['pax_' ~ i ~ '_name']}}
                      </span>
                    </div>
                  {% endif %}

                </li>
                <li class="lastname">
                  <label>Apellido</label>
                  {{ form_errors(form["pax_" ~ i]["lastname"]) }}
                  {{ form_widget(form["pax_" ~ i]["lastname"]) }}
                  {% if ( errores_checkout['pax_' ~ i ~ '_lastname'] is defined ) %}
                    <div class="error ltr active">
                      <span class="left"></span><span class="right">{{errores_checkout['pax_' ~ i ~ '_lastname']}}
                      </span>
                    </div>
                  {% endif %}
                </li>
                <li class="dni">
                  <label>Documento de Viaje</label>
                  {{ form_errors(form["pax_" ~ i]["tipodoc"]) }}
                  {{ form_widget(form["pax_" ~ i]["tipodoc"]) }}
                  {{ form_errors(form["pax_" ~ i]["dni"]) }}
                  {{ form_widget(form["pax_" ~ i]["dni"]) }}
                  {% if ( errores_checkout['pax_' ~ i ~ '_dni'] is defined ) %}
                    <div class="error ltr active">
                      <span class="left"></span><span class="right">{{errores_checkout['pax_' ~ i ~ '_dni']}}
                      </span>
                    </div>
                  {% endif %}
                </li>
                <li class="birth usa-canada">
                  <label>Fecha de nacimiento</label>
                  <ul>
                    <li class="day">
                      {{ form_errors(form["pax_" ~ i]["diafechanac"]) }}
                      {{ form_widget(form["pax_" ~ i]["diafechanac"]) }}
                    </li>
                    <li class="month">
                      {{ form_errors(form["pax_" ~ i]["mesfechanac"]) }}
                      {{ form_widget(form["pax_" ~ i]["mesfechanac"]) }}
                    </li>
                    <li class="year">
                      {{ form_errors(form["pax_" ~ i]["aniofechanac"]) }}
                      {{ form_widget(form["pax_" ~ i]["aniofechanac"]) }}
